;*Set_Start_Val
	exist "初期設定ファイル名.dat"
	if strsize != -1 {
		bload "初期設定ファイル名.dat", nomal, 512	; ファイル名を取得
	} else {
		; 指定するまで作成する必要なし (指定したときに自動的に作成されるから)
			return		; 帰る
	}
	
	if nomal = "" : return		; 何も書かれていない場合はそのまま
	
	exist nomal;+".pf"
	if strsize != -1 {
		vload nomal;+".pf"
		;if mode = 1 : mode = 0	;タブ状態は駄目 ; mode はセーブしない
			return		; 0は成功
	}
	
	; 無かったらそのまま、基本設定で始める。
	dialog "存在しないファイルを指定しているようです。\n　基本設定で始めます。", 0, "報告："+nomal+" はありません"
		return		; -1は失敗